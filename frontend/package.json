    "**/@typescript-eslint/parser": "^4.1.1",
    "node-notifier": "^10.0.1"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "globalSetup": "./test/global-setup.js"
  },
  "_comment": "The 'defaults' and 'not ie 11' are needed to avoid build issues with maplibre-gl.",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "defaults",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
