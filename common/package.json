{
  "name": "common",
  "private": true,
  "version": "0.0.0",
  "description": "Shared Functions and Classes for PRISM",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "build_in_ci": "rm -rf dist && tsc -p tsconfig.json",
    "test:base": "tsx ./src/base/test.ts",
    "test:gml": "tsx ./src/gml/test.ts",
    "test:ows": "tsx ./src/ows/test.ts",
    "test:tsc": "tsc --esModuleInterop --noEmit --skipLibCheck ./src/*.ts ./src/*/*.ts ./src/*/*/*.ts",
    "test:wcs": "npm run test:wcs:utils && npm run test:wcs:class",
    "test:wcs:class": "tsx ./src/wcs/WCS/test.ts",
    "test:wcs:utils": "tsx ./src/wcs/utils/test.ts",
    "test:wfs": "tsx ./src/wfs/test.ts",
    "test:wms": "tsx ./src/wms/utils/test.ts && tsx ./src/wms/WMS/test.ts",
    "test:utils": "tsx ./src/utils/test.ts",
    "test": "npm run test:base && npm run test:gml && npm run test:ows && npm run test:tsc && npm run test:utils && npm run test:wcs && npm run test:wfs && npm run test:wms",
    "audit": "audit-ci --config .audit-ci.json --skip-dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielJDufour/ogc.git"
  },
  "keywords": [
    "test",
    "ogc"
  ],
  "author": "Daniel J. Dufour",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/DanielJDufour/ogc/issues"
  },
  "homepage": "https://github.com/DanielJDufour/ogc#readme",
  "devDependencies": {
    "@types/eslint": "^8.4.6",
    "@types/eslint-scope": "^3.7.4",
    "@types/estree": "^1.0.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^18.7.20",
    "@types/webpack": "^5.28.0",
    "audit-ci": "^7.1.0",
    "cpy-cli": "^4.1.0",
    "cross-fetch": "^3.1.5",
    "find-and-read": "^1.2.0",
    "flug": "^2.3.1",
    "rimraf": "^6.1.2",
    "srvd": "^0.4.0",
    "tsx": "^4.19.2",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "geojson": "^0.5.0",
    "lodash": "^4.17.23",
    "moment": "^2.29.4",
    "xml-utils": "^1.6.0-0"
  },
  "resolutions": {
    "brace-expansion": "^2.0.2"
  },
  "_resolutionsNotes": {
    "brace-expansion": "CVE-2025-5889 - Security fix - Keep resolution: rimraf 6.1.2 → glob → minimatch still uses v2, cannot upgrade to v4 (breaks eslint/minimatch)"
  }
}
