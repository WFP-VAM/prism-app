{
  "name": "prism-frontend",
  "description": "Frontend interface for WFP PRISM",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn run lint --fix"
    ]
  },
  "scripts": {
    "start": "cross-env EXTEND_ESLINT=true react-scripts start",
    "build": "cross-env EXTEND_ESLINT=true react-scripts build",
    "test": "react-scripts test",
    "predeploy:prod": "yarn build",
    "predeploy:staging": "yarn build",
    "deploy:prod": "firebase deploy --only hosting:prod-target",
    "deploy:staging": "firebase deploy --only hosting:staging-target",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src --ignore-path .gitignore",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@material-ui/core": "^4.9.10",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@reduxjs/toolkit": "^1.4.0",
    "@sentry/browser": "^5.15.5",
    "@testing-library/dom": "^6.15.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@turf/bbox": "^6.0.1",
    "@turf/boolean-point-in-polygon": "^6.0.1",
    "@turf/helpers": "^6.1.4",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-eslint": "10.0.3",
    "chart.js": "^2.9.3",
    "colormap": "^2.3.1",
    "d3-fetch": "^1.1.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "^3.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^1.7",
    "geojson": "^0.5.0",
    "geotiff": "^1.0.0-beta.11",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "lodash": "^4.17.15",
    "mapbox-gl": "^1.8.1",
    "moment": "^2.24.0",
    "papaparse": "^5.2.0",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-chartjs-2": "^2.9.0",
    "react-datepicker": "^2.14.1",
    "react-dom": "^16.13.0",
    "react-mapbox-gl": "^4.8.2",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "redux": "^4.0.5",
    "reflect-metadata": "^0.1.13",
    "typescript": "^3.9.7",
    "url": "^0.11.0",
    "xml-js": "^1.6.11"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/chart.js": "^2.9.21",
    "@types/d3": "^5.7.2",
    "@types/geojson": "^7946.0.7",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.149",
    "@types/mapbox-gl": "^1.8.0",
    "@types/node": "^12.0.0",
    "@types/papaparse": "^5.0.3",
    "@types/react": "^16.9.0",
    "@types/react-datepicker": "^2.11.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "cross-env": "^7.0.2",
    "eslint-import-resolver-typescript": "^2.0.0"
  },
  "engines": {
    "node": "12.x"
  }
}
